sString3 = wx.wxString("Hello World !!!")

print "is equal the sString1 and sString2? "
VAL1=sString1.Cmp(sString2.c_str())

printn "\nVAL1=sString1.Cmp(sString2.c_str()): - %1% -\n" % VAL1

if(VAL1 == 0) {
  printn "YES, the sString1 and sString2 are equal."
  OK = 0
} else {
  if(VAL1 > 0) {
    printn "NO, the sString1 is greater than sString2."
  } else  {
    if(VAL1 < 0) {
      printn "NO, the sString1 is less than sString2."
