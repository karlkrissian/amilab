#!/usr/bin/amilab

#SetDebugOn
#SetVerboseOn

NoMessageDialog(1)

# - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - -
# test_wxString_empty.amil
# - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - -
# Check the following methods:
#                              - constructor
#                              - c_str
#                              - Empty
#
# - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - -

func "ctest/ctest_config.amil"

OK = 1 # The test was successful

# - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - -

printn "Create an object of class wxString(sString = wx.wxString(\"hello world !!!\")).\n"
sString = wx.wxString("hello world !!!")

VAL=sString.c_str()

printn "\t Returns the sString value: - %1% -\n" % VAL

printn "Makes the sString empty (sString.Empty().\n"
sString.Empty()

VAL=sString.c_str()

printn "\t Returns the sString value: - %1% -\n" % VAL

if(VAL != "") {
  OK = 0
  printn  "\nERROR: sString no empty."
}

# - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - -

ctest.PrintTestResult( OK ) 

#quit

