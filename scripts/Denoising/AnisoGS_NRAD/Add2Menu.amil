#!/usr/bin/amilab

#
# Adding main Gui scripts to the menu
#

proc AddMenuAnisoGS_NRAD() \
{
  category = "&PDI"
  script = "PDI/AnisoGS_NRAD/AnisoGS_NRADGui.amil"

  # Scripts Menu
