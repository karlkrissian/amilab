#!/usr/bin/amilab

#
# Adding main Gui scripts to the menu
#

proc AddMenuCopyDirAndReplace() \
{
  category = "&Utils"
  script = "Utils/CopyDirAndReplace/CopyDirAndReplaceGui.amil"

  # Scripts Menu
  ami.MainFrame.AddToMenu("&Scripts",category,"&CopyDirAndReplace",script)
}

AddMenuCopyDirAndReplace()
del AddMenuCopyDirAndReplace
