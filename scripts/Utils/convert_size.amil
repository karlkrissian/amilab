#!/home/karl/projects/Sourceforge/builds/amilab/amilab_2.1.0_debug 

ami.MainFrame.ShowWin(0)
printn argc
for n=1 to argc {
  eval "imname = $%1%;" % n
  printn "**** Processing %1%" % imname
  i <<= Image(imname)
  i.info
  i1 <<= Resize(i,(FLOAT) i.tx/4, (FLOAT)i.ty/4, 1,0)
  i1.setvoxelsize(1,1,1)
  i1.save imname-".JPG"+"_2.JPG"
}
quit
