#!/usr/bin/amilab

#
# Adding main Gui scripts to the menu
#

proc AddMenuNewModuleScript() \
{
  category = "&PDI"
  script = "PDI/NewModuleScript/NewModuleScriptGui.amil"

  # Scripts Menu
  ami.MainFrame.AddToMenu("&Scripts",category,"&NewModuleScript",script)
}

AddMenuNewModuleScript()
del AddMenuNewModuleScript
