#!/usr/bin/amilab

#
# Adding main GUI scripts to the menu interface
#

for dir in  "Filtering","Denoising","Noise","Segmentation","SyntheticImages",\
            "Visualization","IO","Centerlines", "QualityMeasures" \
{
  if FileExists(GetScriptsDir+"/"+dir+"/Add2Menu.amil") {
    func dir+"/Add2Menu.amil"
  } else {
    InfoDialog GetScriptsDir+"/"+dir+"/Add2Menu.amil does not exists"
  }
}
#quit
