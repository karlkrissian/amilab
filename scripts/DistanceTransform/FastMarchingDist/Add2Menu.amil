#!/usr/bin/amilab

#
# Adding main Gui scripts to the menu
#

proc AddMenuFastMarchingDist() \
{
  category = "&PDI"
  script = "PDI/FastMarchingDist/FastMarchingDistGui.amil"

  # Scripts Menu
  ami.MainFrame.AddToMenu("&Scripts",category,"&FastMarchingDist",script)
}

AddMenuFastMarchingDist()
del AddMenuFastMarchingDist
