#!/usr/bin/amilab

#
# Adding main Gui scripts to the menu
#

proc AddMenuLocalMeanEvol() \
{
  category = "&PDI"
  script = "PDI/LocalMeanEvol/LocalMeanEvolGui.amil"

  # Scripts Menu
  ami.MainFrame.AddToMenu("&Scripts",category,"&LocalMeanEvol",script)
}

AddMenuLocalMeanEvol()
del AddMenuLocalMeanEvol
