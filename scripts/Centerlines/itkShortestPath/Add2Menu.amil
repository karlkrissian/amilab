#!/usr/bin/amilab

#
# Adding main Gui scripts to the menu
#

proc AddMenuitkShortestPath() \
{
  category = "&Centerlines"
  script = "Centerlines/itkShortestPath/itkShortestPathGui.amil"

  # Scripts Menu
  ami.MainFrame.AddToMenu("&Scripts",category,"itk &Shortest Path",script)
  # ITK menu
  ami.MainFrame.AddToMenu("&ITK",category,"&Shortest Path",script)
}

AddMenuitkShortestPath()
del AddMenuitkShortestPath
