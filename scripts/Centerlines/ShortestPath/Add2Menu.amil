#!/usr/bin/amilab

#
# Adding main Gui scripts to the menu
#

proc AddMenuShortestPath() \
{
  category = "&Centerlines"
  script = "Centerlines/ShortestPath/ShortestPathGui.amil"

  # Scripts Menu
  ami.MainFrame.AddToMenu("&Scripts",category,"&Shortest Path",script)
}

AddMenuShortestPath()
del AddMenuShortestPath
